{
  "VirtualDisplay": {
    "Mode": "Driver",
    "DriverPackage": "path\\to\\virtual-display-driver.inf",
    "VirtualDisplayName": "Virtual Display 1"
  },
  "SunshineConfig": {
    "Policy": "Apply",
    "ConfigPath": "auto-detected",
    "BackupPath": "sunshine.conf.backup"
  },
  "Audio": {
    "VirtualSink": "Steam Streaming Speakers",
    "StreamAudio": true,
    "InstallSteamAudioDrivers": true
  },
  "ResolutionAutomation": {
    "Channel": "PinnedTag",
    "Tag": "v1.0.0"
  },
  "ModeOverrides": [
    "1280x720x60=3840x2160x60",
    "1920x1080x30=1920x1080x60"
  ]
}